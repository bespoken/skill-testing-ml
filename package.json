{
  "name": "skill-testing-ml",
  "version": "0.0.2",
  "description": "Skill Testing Markup Language",
  "main": "index.js",
  "bin": {
    "skill-tester": "./bin/SkillTester.js"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "yml"
    ],
    "silent": true,
    "testMatch": [
      "**/test/*.test.js"
    ],
    "verbose": true
  },
  "scripts": {
    "lint": "eslint --config .eslintrc.json lib/**.js",
    "test": "jest",
    "test.watch": "jest --watchAll"
  },
  "author": "John Kelvie",
  "license": "ISC",
  "dependencies": {
    "cosmiconfig": "^4.0.0",
    "jest": "^22.4.3",
    "js-yaml": "^3.11.0",
    "jsonpath": "^1.0.0",
    "lodash": "^4.17.5",
    "virtual-alexa": "beta"
  },
  "devDependencies": {
    "alexa-sdk": "^1.0.25",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-sort-class-members": "^1.3.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-spellcheck": "0.0.10"
  }
}

{
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/api",
        "recurse": true,
        "readme": "./readme.md"
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["lib"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }

}



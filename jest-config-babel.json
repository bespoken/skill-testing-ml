{
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "dist/lib/**/*.js",
    "dist/bin/**/*.js",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "node"
  ],
  "silent": false,
  "testEnvironment": "node",
  "testMatch": [
    "**/dist/test/**/*.test.js"
  ],
  "reporters": [
    "default",
    "jest-junit"
  ],
  "testURL": "http://localhost/",
  "verbose": true
}
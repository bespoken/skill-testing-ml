{
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.js",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "yml"
  ],
  "silent": true,
  "testRegex": "yml",
  "testRunner": "../../lib/JestAdapter.js",
  "verbose": true
}